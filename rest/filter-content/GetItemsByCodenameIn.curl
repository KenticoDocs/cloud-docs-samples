# Gets three items by their codenames. The codenames are unique per project.
curl --request GET \
  --url 'https://deliver.kontent.ai/<YOUR_ENVIRONMENT_ID>/items?system.codename[in]=delivery_api,get_content,hello_world' \
  --header 'content-type: application/json'
